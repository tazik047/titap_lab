@model DAL.Models.Mark

@{
    ViewBag.Title = "Информация о оценке";
}

<dl class="dl-horizontal">
	@Html.CustomDisplayNameFor(model => model.CriterionId)

	@Html.DisplayFor(model => model.Criterion.Name)

	@Html.CustomDisplayNameFor(model => model.Name)

	@Html.DisplayFor(model => model.Name)

	@Html.CustomDisplayNameFor(model => model.Range)

	@Html.DisplayFor(model => model.Range)

	@Html.CustomDisplayNameFor(model => model.NumMark)

	@Html.DisplayFor(model => model.NumMark)

	@Html.CustomDisplayNameFor(model => model.NormMark)

	@Html.DisplayFor(model => model.NormMark)

</dl>

<button class="btn btn-danger" id="delete-item" type="button" data-deletelink="@Url.Action("Delete", new {id = Model.Id})">Удалить оценку</button>
<a class="btn btn-success" id="edit-item" href="@Url.Action("Manage", new {id = Model.Id})">Редактировать оценку</a>